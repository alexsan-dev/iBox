{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "rootDir": "./src",
    "outDir": "./",
    "sourceMap": true,
    "strict": true,
    "target": "es2017"
  },
  "compileOnSave": true,
  "include":[
    "src/**/*.ts",
  ],
  "exclude": [
     "./node_modules",
     "./next"
  ]
}
